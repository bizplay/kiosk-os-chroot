#! /bin/sh

set -e

case "$1" in
    install)
    ;;

    upgrade)
	if [ ! -L /usr/share/doc/libcupsimage2 ]; then
	    rm -rf /usr/share/doc/libcupsimage2
	fi
    ;;

    abort-upgrade)
    ;;

    *)
        echo "preinst called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac



exit 0
